{
  "autoDocstring.docstringFormat": "sphinx",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "editor.fontFamily": "'agave Nerd Font', monospace",
  "editor.fontSize": 15,
  "C_Cpp.default.includePath": ["/opt/ros/melodic/include"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "window.titleBarStyle": "custom",
  "vim.cursorStylePerMode.insert": "underline",
  "vim.cursorStylePerMode.normal": "block-outline",
  "window.menuBarVisibility": "toggle",
  "editor.tabSize": 2,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "vim.foldfix": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [80, 120],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#aaaaaa27"
  },
  "zenMode.hideLineNumbers": false,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 80,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "zenMode.hideStatusBar": false,
  "zenMode.centerLayout": false,
  "clock.alignment": "Right",
  "clock.format": "dd/mm/yyyy HH:MM",
  "vim.useSystemClipboard": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "latex-workshop.view.pdf.viewer": "external",
  "clang-format.language.java.enable": false,
  "clang-format.language.javascript.enable": false,
  "clang-format.language.objective-c.enable": false,
  "clang-format.language.objective-cpp.enable": false,
  "clang-format.language.typescript.enable": false
}
